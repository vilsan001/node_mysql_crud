<body>
    <div style="margin-bottom: 20 px;" class="main"><a href="./">go to main page</a></div><br>

<DIV align="center" style="margin-bottom: 20px;"><h1>search with address</h1>
<form method="get" action="/searchdata">
<input style="display: flex;" type="search" name="search" placeholder="search here"><br><button>search</button></form><br>
{{#if msg1}}

<h1>found</h1>

 <div>
    <table class="table">
  <thead>
    <tr>
      
      <th scope="col">name</th>
      <th scope="col">classs</th>
      <th scope="col">address</th>
    </tr>
  </thead>
  <tbody>
   
   {{#data}}   
    <tr>
      
      <td>{{name}}</td>
      <td>{{classs}}</td>
      <td>{{address}}</td>
    </tr>

    {{/data}}
  </tbody>
</table>
</div> 



{{/if}}


{{#if msg2}}
<h1>no found</h1>
{{/if}}
</DIV>
</body>
</html>





